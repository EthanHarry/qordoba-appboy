{
  "manifest_version": 2,
  "name": "Qordoba - Appboy",
  "version": "1.0",
  "description": "Allows to easily send / receive content between Appboy and Qordoba",
  "page_action": {
    "default_title": "Qordoba - Appboy",
    "default_icon": "./assets/logo.png"
    // "default_popup": "./popup.html"
  },
  "background": {
    "scripts": ["domScript.bundle.js", "inject.bundle.js"],
    "persistent": false
  },
  "permissions": ["activeTab", "tabs"], //, "https://dashboard.appboy.com/*"
  "update_url": "http://0.0.0.0"
}